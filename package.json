    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "atob": "^2.1.2",
    "classnames": "2.2.6",
    "babel-loader": "^8.0.6",
    "blake2s": "^1.1.0",
    "bootstrap": "^3.4.1",
    "bs58": "^4.0.1",
